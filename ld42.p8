function make_smoke(startx, starty)
 

   local smoke_particle = {
  
   x=startx,
   y=starty,
  
   t = 0,
 
   life_time=20+rnd(10),

   size = 1,
   max_size = 1+rnd(3),
   
   dy = rnd(0.7) * -1,
   dx = rnd(0.4) - 0.2,

   ddy = -0.05,
 
   col = 7
 }

 add(smoke,smoke_particle)
end

function update_smoke()
  for p in all(smoke) do

    p.y += p.dy
    p.x += p.dx
    p.dy+= p.ddy
    p.t += 1/p.life_time

    p.size = max(p.size, p.max_size * p.t )

    if p.t > 0.7  then
      p.col = 6
    end
    if p.t > 0.9 then
      p.col = 5
    end
    
    if p.t > 1 then
      del(smoke,p)
    end
  end
end

function draw_smoke()
  for p in all(smoke) do
    circfill(p.x, p.y, p.size, p.col)
  end
end


