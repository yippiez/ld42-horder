function make_object(x,y,id)
 local o = {}
 local ddx = 0--delay in x because of map
 if(scene=="tutorial") ddx = 256
 if(scene=="game") ddx = 384
 o.dx,o.dy=0,0
 o.x = x + ddx
 o.y = y
 o.id = id
 if (o.id=="box") o.i = 7
 if (o.id=="trashcan") o.i = 34 o.storage = 0
 if (o.id=="table") o.i = 36
 if (o.id=="trash1") o.i = 7
 if (o.id=="trash2") o.i = 8
	if (o.id=="solderpen") o.i = 33 o.jobs = 0
	if (o.id=="racoon") o.i = 16
	puff(o.x+4,o.y+4)
	add(objects,o)
	return o
end

