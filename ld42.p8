--particles
function newpart(x,y,c)
local p={}
p.x=x
p.y=y
p.c=c
p.dx= rnd(16)*.1 -.8
p.dy= rnd(10)*.1 -.5
p.dt=0
p.size=2
add(particles,p)
end

function updatepart(o)
--dt time since the spawn
o.dt+=1
if(o.dt>15) o.dx=0 o.dy=0 o.size=1
if(o.dt>30) o.size=1
if(o.dt>45) o.size=0
if(o.dt>60) del(particles,o)

o.x+=o.dx*1.1
o.y+=o.dy*1.1
end



function dpart(o)
circfill(o.x,o.y,o.size,o.c)
end

function puff(x,y)
for i=1,rnd(8)+10 do
		local random=flr(rnd(2)+1)
		local clr = {6,7}
		newpart(x,y,clr[random])
	end
end
