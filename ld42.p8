--collision box
function cmap(o,flag)
  local ct = {}
  -- if colliding with map tiles
    local x1=o.x/8 -- left
    local y1=o.y/8-- up 
    local x2=(o.x+7)/8 -- right
    local y2=(o.y+8)/8 -- down
    local a=fget(mget(x1,y1),flag)
    local b=fget(mget(x1,y2),flag)
    local c=fget(mget(x2,y2),flag)
    local d=fget(mget(x2,y1),flag)
    ct={a,b,c,d}
  return ct 
end

function checkleft(o,flag)
local col = cmap(johny,flag)
return not (col[1] and col[2])
end

function checkup(o,flag)
local col = cmap(johny,flag)
return not (col[1] and col[4])
end

function checkright(o,flag)
local col = cmap(johny,flag)
return not (col[3] and col[4])
end

function checkdown(o,flag)
local col = cmap(johny,flag)
return not (col[2] and col[3])
end


function collide(c1,c2,w)
	if(c1.x > c2.x+w) return f --left
	if(c1.x+w < c2.x) return f --right    
	if(c1.y+w < c2.y) return f --down
	if(c1.y > c2.y+w) return f --up      
 	return t
end


